# API Keys for Cybersecurity Council
# Copy this file to .env and fill in your actual API keys
# Leave environment variables empty if you do not want to use them

# VirusTotal API Key
# Get your free API key from: https://www.virustotal.com/gui/my-apikey
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# AbuseIPDB API Key  
# Get your free API key from: https://www.abuseipdb.com/account/api
ABUSEIPDB_API_KEY=your_abuseipdb_api_key_here

# OpenAI API Key (for AG2 agents)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=
OPENAI_MODEL=

# Google Gemini API Key (for AG2 agents)
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=
GEMINI_MODEL=

# Ollama Configuration (for local LLMs)
# Base URL for Ollama API (default: http://localhost:11434)
OLLAMA_BASE_URL=
# Model to use with Ollama (e.g., llama2, mistral, codellama)
OLLAMA_MODEL=
